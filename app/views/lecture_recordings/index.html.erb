<h1>Listing lecture_recordings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lecture_recordings.each do |lecture_recording| %>
      <tr>
        <td><%= lecture_recording.name %></td>
        <td><%= lecture_recording.user_id %></td>
        <td><%= link_to 'Show', lecture_recording %></td>
        <td><%= link_to 'Edit', edit_lecture_recording_path(lecture_recording) %></td>
        <td><%= link_to 'Destroy', lecture_recording, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lecture recording', new_lecture_recording_path %>
